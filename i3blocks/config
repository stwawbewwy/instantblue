[volume]
command=~/.config/i3blocks/volume.sh
interval=30
signal=10

[music]
command=~/.config/i3blocks/music.sh
interval=persist

[brightness]
command=~/.config/i3blocks/brightness.sh
interval=once
signal=20

[battery]
command=~/.config/i3blocks/battery.sh
interval=persist

[date]
command=echo "󰃭 $(date +"%a %d-%m-%y")"
interval=300

[time]
command=echo "󰥔 $(date +"%R")"
interval=1
